{
  "name": "big-plant",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node /dist/server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install:client": "cd client && npm install",
    "build:server": "babel server --out-dir dist",
    "build:client": "cd client && npm run build",
    "build": "npm run build:server && npm run install:client && npm run build:client",
    "seed": "cd server && cd config && node scripts/seedDB",
    "restart": "npm run build:server && npm run start",
    "heroku-postbuild": "npm run build",
    "rebuild:nodeSass": "cd client && npm rebuild node-sass"
  },
  "author": "Mark Awai",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-core": "^6.26.3",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16"
  }
}
